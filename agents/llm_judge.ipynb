    "from typing import List, Dict, Any\n",
    "from graph.graph import app\n",
    "\n",
    "async def run_agent_and_collect(\n",
    "    queries: List[str],\n",
    ") -> List[Dict[str, Any]]:\n",
    "    runs: List[Dict[str, Any]] = []\n",
    "\n",
    "    for q in queries:\n",
    "        result = await app.ainvoke({\"input\": q})\n",
    "        user_input = q\n",
    "        final_response = result[\"response\"]\n",
    "        retrieved_contexts = result[\"knowledge\"]\n",
    "\n",
    "        if not isinstance(retrieved_contexts, list):\n",
    "            retrieved_contexts = [str(retrieved_contexts)]\n",
    "\n",
    "        runs.append(\n",
    "            {\n",
    "                \"user_input\": user_input,\n",
    "                \"response\": final_response,\n",
    "                \"retrieved_contexts\": retrieved_contexts,\n",
    "            }\n",
    "        )\n",
    "\n",
    "    return runs\n",
    "\n",
    "test_queries = [\n",
    "    \"I want to find a stuffed animal for kids under $30. What are the best cute options?\",  # price constraint\n",
    "    \"I’m looking for an eco-friendly wooden toy under 15 dollars. What’s the best option?\", \n",
    "    # eco-friendly constraint + price\n",
    "    \"Do you have any cheap LEGO sets currently available?\",  \n",
    "    # brand + alternative\n",
    "    \"I want to find a LEGO for kids under 8 years old. What are some options?\",  \n",
    "    # ingredient/safety but toy domain\n",
    "    \"toy made of wood that’s currently in stock?\",  \n",
    "    # availability check + toy category\n",
    "    \"What’s the current price of a popular natural slime-making kit? Anything under $20 in stock?\",  \n",
    "    # price + availability\n",
    "    \"I want to find a Halloween costume for kids ages 8–14. Any recommendations?\",  \n",
    "    # age-range constraints\n",
    "    \"Compare two good remote-control cars for kids—one durable budget model and one high-performance premium option.\",  \n",
    "    # comparison request\n",
    "    \"Can you recommend something safe for toddlers to play with while teething?\",  \n",
    "    # safety-focused toy query\n",
    "    \"I need a toy that helps calm a child after a nightmare—what kind of soothing toy would work?\",  \n",
    "    # specific use-case (analogous to wine-stain query but toy domain)\n",
    "]\n",
    "\n",
    "runs = await run_agent_and_collect(test_queries)\n",
    "len(runs)"
   ]
